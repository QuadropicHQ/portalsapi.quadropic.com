generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  username  String   @id @unique
  name      String
  email     String   @unique
  passkey   Json?
  dob       DateTime?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  sessions  String[]
}

model Challenge {
  uid       String   @id @unique
  challenge String
  ipaddress String
  priority  Boolean?
}

model EmailChallenge {
  uid       String   @id @unique
  challenge String
  ipaddress String
  priority  Boolean?
}
